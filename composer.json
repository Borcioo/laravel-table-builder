{
    "name": "borek/laravel-table-builder",
    "description": "Table builder package for Laravel 11 applications with Inertia and React",
    "version": "1.0.0",
    "type": "library",
    "license": "GPL-3.0-or-later",
    "keywords": [
        "laravel",
        "table",
        "builder",
        "inertia",
        "react"
    ],
    "autoload": {
        "psr-4": {
            "Borek\\LaravelTableBuilder\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Borek\\LaravelTableBuilder\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Borek",
            "email": "m.barta@foreto.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.2",
        "illuminate/database": "^11.0",
        "illuminate/support": "^11.0",
        "illuminate/contracts": "^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "orchestra/testbench": "^9.0",
        "laravel/pint": "^1.13"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Borek\\LaravelTableBuilder\\Providers\\TableBuilderServiceProvider"
            ],
            "aliases": {
                "TableBuilder": "Borek\\LaravelTableBuilder\\Facades\\TableBuilder"
            }
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "format": "vendor/bin/pint"
    }
}
